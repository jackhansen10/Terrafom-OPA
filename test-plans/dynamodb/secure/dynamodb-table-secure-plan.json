{
  "format_version": "1.2",
  "terraform_version": "1.5.7",
  "resource_changes": [
    {
      "address": "aws_dynamodb_table.this",
      "mode": "managed",
      "type": "aws_dynamodb_table",
      "name": "this",
      "provider_name": "aws",
      "change": {
        "actions": ["create"],
        "after": {
          "name": "secure-dynamodb-table",
          "billing_mode": "PROVISIONED",
          "read_capacity": 5,
          "write_capacity": 5,
          "hash_key": "id",
          "attribute": [
            {
              "name": "id",
              "type": "S"
            }
          ],
          "server_side_encryption": {
            "enabled": true,
            "kms_key_id": "arn:aws:kms:us-east-1:123456789012:key/12345678-1234-1234-1234-123456789012"
          },
          "point_in_time_recovery": {
            "enabled": true
          },
          "deletion_protection": {
            "enabled": true
          },
          "backup": {
            "enabled": true
          },
          "stream_enabled": true,
          "stream_view_type": "NEW_AND_OLD_IMAGES",
          "ttl": {
            "enabled": true,
            "attribute_name": "ttl"
          },
          "tags": {
            "Name": "secure-dynamodb-table",
            "Purpose": "secure-database"
          }
        }
      }
    }
  ]
}
